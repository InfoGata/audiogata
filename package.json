{
  "name": "AudioGata",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "build/electron/main.js",
  "dependencies": {
    "@dnd-kit/core": "^5.0.1",
    "@dnd-kit/sortable": "^6.0.0",
    "@dnd-kit/utilities": "^3.1.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.2.1",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.25",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.16",
    "@types/webpack-env": "^1.16.0",
    "@types/wicg-mediasession": "^1.1.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.0.0",
    "customize-cra": "^1.0.0",
    "dexie": "^3.0.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "node-fetch": "^3.2.3",
    "notistack": "^2.0.3",
    "plugin-frame": "0.3.0",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cors-server": "node servers/cors/server.js",
    "postinstall": "electron-builder install-app-deps && cd servers/cors && npm install",
    "dev": "concurrently --kill-others \"npm run cors-server\" \"npm start\"",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "wait-on": "^6.0.1"
  }
}
