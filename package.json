  "name": "audiogata",
  "version": "0.1.0",
  "private": true,
  "homepage": "",
  "main": "build/electron/main.js",
  "dependencies": {
    "@awesome-cordova-plugins/music-controls": "^5.44.0",
    "@capacitor/android": "3.6.0",
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "^3.6.0",
    "@datapunt/matomo-tracker-react": "^0.5.1",
    "@dnd-kit/core": "^5.0.1",
    "@dnd-kit/sortable": "^6.0.0",
    "@dnd-kit/utilities": "^3.1.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^2.3.3",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^27.5.2",
    "@types/mui-image": "^1.0.1",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.16",
    "@types/webpack-env": "^1.16.0",
    "@types/wicg-mediasession": "^1.1.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "concurrently": "^6.0.0",
    "cordova-plugin-fetch": "git+https://git@github.com/aporat/cordova-plugin-fetch.git",
    "cordova-plugin-music-controls2": "^3.0.6",
    "customize-cra": "^1.0.0",
    "dexie": "^3.0.3",
    "dompurify": "^2.3.8",
    "electron-devtools-installer": "^3.2.0",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "hls.js": "^1.2.4",
    "i18next": "^22.0.2",
    "i18next-browser-languagedetector": "^6.1.8",
    "lodash": "^4.17.21",
    "mui-image": "^1.0.7",
    "node-fetch": "^3.2.3",
    "notistack": "^2.0.3",
    "plugin-frame": "^0.5.0",
    "react": "^18.2.0",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-query": "^3.39.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "cors-server": "node servers/cors/server.js",
    "postinstall": "electron-builder install-app-deps && cd servers/cors && npm install",
    "dev": "concurrently --kill-others \"npm run cors-server\" \"npm start\"",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm run build && tsc -p electron && electron-builder",
    "cap:build": "npm run build && cap sync",
    "android": "npm run build && cap run android"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "plugins": [
      "jam3"
    ],
    "rules": {
      "jam3/no-sanitizer-with-danger": 2
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^3.5.0",
    "@types/lodash": "^4.14.182",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "eslint-plugin-jam3": "^0.2.3",
    "wait-on": "^6.0.1"
  }
}
